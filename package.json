{
  "name": "@danydodson/eslint-config-react",
  "version": "1.3.0",
  "description": "ESLint React configuration for JS projects based on Prettier configs",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write **/*.js",
    "lint": "eslint ./ --ignore-path .gitignore",
    "lint:fix": "eslint . --fix",
    "release": "standard-version"
  },
  "prettier": "@danydodson/prettier-config",
